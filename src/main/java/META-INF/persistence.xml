<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="Experimental">
	 <class>example.test.backend.data.Product</class>
	 <class>example.test.backend.data.Category</class>

    <properties>
        <property name="javax.persistence.jdbc.url" value="jdbc:sqlserver://localhost:1434;databaseName=InventoryTest;integratedSecurity=true"/>
        <property name="javax.persistence.jdbc.user" value="ccummings"/>
        <property name="javax.persistence.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
        <property name="eclipselink.cache.shared.default" value="false"/>
    </properties>
	</persistence-unit>
</persistence>
